rofi.separator-style: none
rofi.sidebar-mode: true
rofi.bw: 1
rofi.columns: 1
rofi.padding: 16

rofi.yoffset: 0
rofi.fake-transparency: false
rofi.location: 0
rofi.width: 500
rofi.hide-scrollbar: true
rofi.font: Monaco 12
rofi.lines: 12
rofi.fixed-num-lines: true



rofi.color-enabled: true
rofi.color-window: #bf0A161B, #bf0A161B, #00000000
rofi.color-normal: #00000000, #d6e7f3, #00000000, #1287BC, #0A161B
rofi.color-active: #00000000, #d6e7f3, #00000000, #1287BC, #0A161B
rofi.color-urgent: #00000000, #d6e7f3, #00000000, #1287BC, #0A161B
